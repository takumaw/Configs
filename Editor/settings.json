{
    "black-formatter.importStrategy": "fromEnvironment",
    "debug.autoExpandLazyVariables": true,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.openDebug": "openOnSessionStart",
    "diffEditor.codeLens": true,
    "diffEditor.experimental.showMoves": true,
    "diffEditor.experimental.useVersion2": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.linkedEditing": true,
    "editor.suggest.preview": true,
    "explorer.autoRevealExclude": {
        "**/.*_cache": true,
        "**/.cache": true,
        "**/.venv": true
    },
    "explorer.compactFolders": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoGuessEncoding": true,
    "files.autoSave": "afterDelay",
    "files.readonlyFromPermissions": true,
    "files.simpleDialog.enable": true,
    "git.autofetch": true,
    "git.autoStash": true,
    "git.confirmSync": false,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.fetchOnPull": true,
    "git.followTagsWhenSync": true,
    "git.mergeEditor": true,
    "git.pruneOnFetch": true,
    "git.rebaseWhenSync": true,
    "git.timeline.showUncommitted": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.themeMatplotlibPlots": true,
    "markdown.validate.enabled": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.lineNumbers": "on",
    "notebook.output.scrolling": true,
    "notebook.output.wordWrap": true,
    "notebook.stickyScroll.enabled": true,
    "python.analysis.autoImportCompletions": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.downloadStubs": true,
    "python.analysis.gotoDefinitionInStringLiteral": true,
    "python.analysis.inlayHints.callArgumentNames": true,
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": true,
    "remote.downloadExtensionsLocally": true,
    "remote.SSH.enableDynamicForwarding": false,
    "remote.tunnels.alwaysUpdateCLI": true,
    "scm.defaultViewMode": "tree",
    "scm.repositories.sortOrder": "name",
    "search.exclude": {
        "**/.*_cache": true,
        "**/.cache": true,
        "**/.venv": true
    },
    "search.experimental.closedNotebookRichContentResults": true,
    "search.showLineNumbers": true,
    "search.useGlobalIgnoreFiles": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.enableBell": true,
    "terminal.integrated.persistentSessionScrollback": 10000,
    "terminal.integrated.scrollback": 100000,
    "update.showReleaseNotes": false,
    "vsicons.dontShowNewVersionMessage": true,
    "window.commandCenter": true,
    "window.dialogStyle": "custom",
    "window.newWindowDimensions": "maximized",
    "workbench.colorTheme": "Default Dark+",
    "workbench.iconTheme": "vscode-icons",
    "workbench.preferredDarkColorTheme": "Default Dark+",
    "workbench.preferredLightColorTheme": "Default Light+",
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true,
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "xml.codeLens.enabled": true,
}